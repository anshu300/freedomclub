                <script type="text/javascript">
                    $('#Sub').on("click", function () {
                        var username=$('#username').val();
                        var email=$('#email').val();
                        var mobile=$('#mobile').val();
                        var state=$('#state').val();
                        var city=$('#city').val();
                        var msg=$('#msg').val();
                        var id=$('#pid').val();
                        var state;
                        if(username==""){
                            alert("please enter your username");
                            return false;
                        }
                        if(email==""){
                            alert("please enter your username");
                            return false;
                        }
                        if(mobile==""){
                            alert("please enter your username");
                            return false;
                        }
                        $('select').on("change",function(){
                            state=$(this).val();
                        });
                        if(state==""){
                            alert("please enter your username");
                            return false;
                        }
                      $.post("/products/enqiry",{"product_id":id,"username":username,"email":email,"mobile":mobile,"state":state,"city":city,"msg":msg},function(d){
                          console.log(d);
                      });  
                    });
                </script>


