                <script type="text/javascript">
                  var rtype = "Prepaid";
                  var oprator;
                  var recharge = "mobile";
                  $('.text-center').on("click", function () {
                    recharge = $(this).data().id;
                  });
                  $('.chk').on("click", function () {
                    rtype = $(this).val();
                  });
                  $('select').on("change", function () {
                    oprator = $(this).val();
                  });
                  $('#Rechage').on("click", function () {
                      var amount = $('#amount').val();
                      var mobile = $('#mobile_num').val();
                      if(amount==""){
                         alert("Please Enter Amount");
                          return false;
                      }
                      if (mobile == "") {
                        alert("Please Enter A Mobile Number");
                        return false;
                      }
                      if (/^\d{10}$/.test(mobile)) {
                      } else {
                        alert("Invalid number");
                        return false;
                      }
                    var uid = $('#uid').val();
                    if (uid == 0) {
                      $('#Loginmodal').modal('show');
                      $('#SaveChange').on("click", function () {
                        var mob = $('#mobile_number').val();
                        var pass = $('#modal_Password').val();
                        $.post("/ajaxlogin", { "mobile": mob, "password": pass }, function (d) {
                          if (d.status == 200) {
                            $('#uid').val(1);
                            $('#Loginmodal').modal('hide');
                            $.post("/users/recharge", { "mobile": mobile, "amount": amount, "oprator": oprator, "r_type": rtype, "rechage_type": recharge }, function (d) {
                             window.location.href = "/recharge-payment";
                           });
                          }
                          if (d.status == 500) {
                            $('.error-mess').html("Your User Name And Password Is not Correct");
                          }
                        });
                      });
                    } else {
                      $.post("/users/recharge", { "mobile": mobile, "amount": amount, "oprator": oprator, "r_type": rtype, "rechage_type": recharge }, function (d) {
                        window.location.href = "/recharge-payment";
                      });
                      // var url = "http://api.rechapi.com/recharge.php?format=json&token=R5eWtAEIYqJWQFlHFwQeNco5cZpUWC&mobile=9616499322&amount=10&opid=3&urid=66C02462A2211022C&opvalue1=#opvalue1&opvalue2=#opvalue2";
                      // $.get(url, function (d) {
                      //   console.log(d);
                      // });
                    }
                  });
                  $('.browsePlan').on("click", function () {
                    $('.tlb').html(" ");
                    $('#myModal').modal('show');
                    $.get("https://api.rechapi.com/rech_plan.php?format=json&token=R5eWtAEIYqJWQFlHFwQeNco5cZpUWC&type=3G&cirid=1&opid=" + oprator, function (d) {
                      data = JSON.parse(d);
                      console.log(d);
                      var d = data.data;
                      for (i in d) {
                        var arr = d[i];
                      }
                      for (i in arr) {
                        console.log(arr[i]);
                        $('<tr><td>' + arr[i].talktime + '</td><td>' + arr[i].detail + '</td><td>' + arr[i].validity + '</td><td><button type="button" class="btn btn-outline-secondary" id="Recharge_amount" data-id="' + arr[i].amount + '">' + arr[i].amount + '</button></td></tr>').appendTo('.4G');
                      }
                    });
                  });

                  $("#mobile_num").keyup(function (e) {
                    var val = $(this).val();
                    if (isNaN(val)) {
                      console.log("str");
                    } else {
                      if(val.length==4){
                        console.log(val);
                         my(val);
                      } 
                    }
                    if (e.keyCode == 8) {
                      var val = $('#mobile_num').val();
                      if(val.lenght==4){
                        my(val);
                      }
                    }
                  });

                function my(val) {
                    console.log(val);
                    $.post("/detail", { "service": val }, function (d) {
                      console.log(d);
                        if(d.status==500){

                        }
                        else {
                          $('#rechare-card').html(" ");
                          oprator = d.data[0].operator_code;
                          console.log(d);
                          $('<option value="' + d.data[0].operator_code + '">' + d.data[0].service + '</option>').appendTo('#rechare-card');
                        }
                      });
                  }
                

                  $(document).on("click", "#Recharge_amount", function () {
                    var amount = $(this).data().id;
                    $('#amount').val(amount);
                    $('#myModal').modal('hide');
                  });
                    
                  $.get("/operator",function(d){
                     var data=d.data;
                     for(i in data){
                       $('<option value="'+data[i].operator_code+'">'+data[i].service+'</option>').appendTo('#rechare-card');
                     }
                  });

                  $('.dth-recharge').on("click", function () {
                   
                  });
